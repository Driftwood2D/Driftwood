{
  "database": {
    "root": "db/",
    "name": "default.ubj"
  },
  "cache": {
    "ttl": 300
  },
  "input": {
    "keybinds": {
      "up": "UP",
      "down": "DOWN",
      "left": "LEFT",
      "right": "RIGHT",
      "interact": "SPACE",
      "face": "LSHIFT",
      "console": "BACKQUOTE"
    },
    "debug": true
  },
  "audio": {
    "frequency": 22050,
    "music_volume": 128,
    "sfx_volume": 128
  },
  "log": {
    "verbose": true,
    "halt": true,
    "file": "driftwood.log",
    "suppress": [
      ["Tick"],
      ["Cache", "clean", "referrers"]
    ],
    "suppress_halt": [
      ["WARNING"]
    ]
  },
  "path": {
    "root": "data/",
    "path": [
      "blue/"
    ]
  },
  "window": {
    "title": "Driftwood 2D",
    "width": 480,
    "height": 480,
    "fullscreen": false,
    "zoom": 4,
    "maxfps": 0
  }
}
